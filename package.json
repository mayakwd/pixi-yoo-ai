{
  "name": "pixi-yoo-ai",
  "version": "0.0.27",
  "description": "Yoo Ai. Extensive UI library for PixiJS v5",
  "author": "Ilya Malanin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayakwd/pixi-yoo-ai.git"
  },
  "bugs": {
    "url": "https://github.com/mayakwd/pixi-yoo-ai/issues"
  },
  "homepage": "https://github.com/mayakwd/pixi-yoo-ai#readme",
  "keywords": [
    "pixi",
    "ui",
    "gui"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "moduleResolution": "node",
  "scripts": {
    "setup": "yarn install",
    "lint": "yarn tslint --fix -c ./tslint.json 'src/**/*.ts'",
    "build": "tsc",
    "test": "jest"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "pixi.js": "^5.3.3",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "gsap": "^3.5.0"
  },
  "peerDependencies": {
    "pixi.js": "^5.3.3",
    "gsap": "^3.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|ts)|**/__tests__/*.(js|ts)"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
