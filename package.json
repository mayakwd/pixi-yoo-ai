{
  "name": "pixi-yoo-ai",
  "version": "1.0.3",
  "description": "Yoo Ai. Extensive UI library for PixiJS v6",
  "author": "Ilya Malanin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayakwd/pixi-yoo-ai.git"
  },
  "bugs": {
    "url": "https://github.com/mayakwd/pixi-yoo-ai/issues"
  },
  "homepage": "https://github.com/mayakwd/pixi-yoo-ai#readme",
  "keywords": [
    "pixi",
    "ui",
    "gui"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "moduleResolution": "node",
  "scripts": {
    "setup": "yarn install",
    "lint": "yarn eslint ./src/ --ext .ts",
    "lint-fix": "yarn eslint ./src/ --ext .ts",
    "prettify": "yarn prettier --config .prettierrc ./src/**/*.ts --write",
    "build": "tsc",
    "test": "jest"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "@pixi/core": "^6.2.0",
    "@pixi/math": "^6.2.0",
    "@pixi/display": "^6.2.0",
    "@pixi/canvas-display": "^6.2.0",
    "@pixi/utils": "^6.2.0",
    "@pixi/text": "^6.2.0",
    "@pixi/interaction": "^6.2.0",
    "@pixi/sprite": "^6.2.0",
    "@pixi/graphics": "^6.2.0",
    "@pixi/loaders": "^6.2.0",
    "@pixi/app": "^6.2.0",
    "ts-jest": "^26.2.0",
    "eslint": "^8.6.0",
    "typescript": "^4.5.4",
    "gsap": "^3.5.0",
    "tslib": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1"
  },
  "peerDependencies": {
    "@pixi/core": "^6.2.0",
    "@pixi/math": "^6.2.0",
    "@pixi/display": "^6.2.0",
    "@pixi/utils": "^6.2.0",
    "@pixi/text": "^6.2.0",
    "@pixi/interaction": "^6.2.0",
    "@pixi/sprite": "^6.2.0",
    "@pixi/graphics": "^6.2.0",
    "@pixi/loaders": "^6.2.0",
    "@pixi/app": "^6.2.0",
    "gsap": "^3.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettify && lint"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|ts)|**/__tests__/*.(js|ts)"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
